{
  "expo": {
    "name": "insta-story",
    "slug": "insta-story",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/instagram.png",
    "splash": {
      "image": "./assets/instagram.png",
      "resizeMode": "contain",
      "backgroundColor": "#111"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/instagram.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/instagram.png"
    },
    "description": "this is instagram clone",
    "githubUrl": "https://github.com/jay75chauhan/insta-story-app"
  }
}
